#!/usr/bin/rC
import check_number

function guess();{
    newln
    num tries >>1
    for l in range:999999; name:for1; indent:"  ";{
      newln
      Write "Enter your guess: "
      num guess_ >>$readline
      check_number()
    }for1;
}guess;

function main();{
    Write "Guess the number from 1-100"
    num random_number >>rand:1,100
    guess()
}main;

main()
newln
exit()
